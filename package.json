  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrewc2020/students/issues"
  },
  "homepage": "https://github.com/andrewc2020/students#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-async-to-generator": "^7.7.0",
    "@babel/preset-env": "^7.6.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "express": "^4.16.4",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
